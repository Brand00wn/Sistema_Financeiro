@model IEnumerable<Financeiro.Models.Tipo>

@{
    ViewBag.Title = "Tipos";
}

<h2>Tipos</h2>

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <input type="button" class="btn btn-default" value="Voltar" onclick="location.href='@Url.Action("Create", "Tipo")'" />
        </div>
    </div>
</div><br>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tipo1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo1)
        </td>
        <td>
            @Html.ActionLink("Alterar", "Edit", new { id=item.idTipo }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.idTipo })
        </td>
    </tr>
}

</table>
